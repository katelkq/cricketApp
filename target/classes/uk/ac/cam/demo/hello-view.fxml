        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

          <Label fx:id="welcomeText">
      <font>
         <Font name="SF Pro Display Bold" size="48.0" />
      </font>
   </Label>
   <ScrollPane hbarPolicy="NEVER" prefHeight="400.0" prefWidth="200.0">
     <content>
       <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="700.0" prefWidth="350.0" />
     </content>
   </ScrollPane>
   <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
     <tabs>
       <Tab text="Untitled Tab 1">
         <content>
           <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                  <children>
                      <Button layoutX="74.0" layoutY="67.0" onAction="#onHelloButtonClick" style="-fx-background-color: #679BF1;" text="Hello!" />
                  </children>
               </AnchorPane>
         </content>
       </Tab>
       <Tab text="Untitled Tab 2">
         <content>
           <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
